{
  "name": "Eli",
  "description": "Eli is frontend development template for me.(include Honoka)",
  "main": "Gruntfile.js",
  "authors": [
    "windyakin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/windyakin/Eli",
  "moduleType": [],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "jquery": "~2.1.4",
    "Honoka": "3.3.7",
    "bootstrap-sass": "3.3.7",
    "bootstrap": "3.3.7"
  },
	"overrides": {
		"Honoka": {
			"main": []
		},
		"bootstrap": {
			"main": ["dist/fonts/**/*", "dist/js/bootstrap.**js"]
		},
		"bootstrap-sass": {
			"main": []
		},
		"jquery": {
			"main": ["dist/jquery.**js"]
		}
  },
  "exportsOverride": {
    "jquery": {
      ".": "dist/jquery**.js"
    },
    "Honoka": {},
    "bootstrap-sass": {},
    "bootstrap": {
      "js": "dist/js/bootstrap.**js",
      "fonts": "dist/fonts/**/*"
    }
  }
}
